<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.palande.thefitness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScrollingArmsActivity"
            android:label="@string/title_activity_scrolling_arms"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScrollingAbsActivity"
            android:label="@string/title_activity_scrolling_abs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UpperBodyContentActivity"
            android:label="@string/title_activity_upper_body_content"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UpperBodyScrollingActivity"
            android:label="@string/title_activity_upper_body_scrolling"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>